{
  "name": "nwb-assemble",
  "version": "1.0.0",
  "description": "Describe nwb-assemble here",
  "private": true,
  "scripts": {
    "build": "nwb build-web-app && assemble",
    "clean": "nwb clean-app",
    "start": "nwb serve-web-app --reload",
    "test": "nwb test",
    "test:coverage": "nwb test --coverage",
    "test:watch": "nwb test --server",
    "serve": "serve dist",
    "watch": "npm-watch"
  },
  "watch": {
    "build": {
      "patterns": [
        "src",
        "src/views"
      ],
      "extensions": "hbs,json,js,scss"
    }
  },
  "devDependencies": {
    "assemble": "^0.24.3",
    "gulp-extname": "^0.2.2",
    "gulp-inject": "^4.3.0",
    "handlebars-helpers": "^0.9.8",
    "jsonfile": "^4.0.0",
    "manifest-webpack-plugin": "^0.2.0",
    "npm-watch": "^0.3.0",
    "nwb": "0.18.x",
    "pretty": "^2.0.0",
    "serve": "^6.4.1",
    "webpack-manifest-plugin": "^1.3.2"
  },
  "author": "",
  "license": "MIT",
  "repository": ""
}
